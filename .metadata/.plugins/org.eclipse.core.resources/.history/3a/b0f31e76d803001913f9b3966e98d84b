#Queue using array
class Queue:
    
    def __init__(self):
        self.q = [];        
        
    def enqueue(self,data):
        self.q.append(data);
        
    def dequeue(self):
        return self.q.pop(0);
    
    def size(self):
        return self.q.__len__();
        
    def isEmpty(self):
        return len(self.q)==0;
        
queue = Queue();

queue.enqueue("Alpha")
queue.enqueue("Beta")
queue.enqueue("Gamma")
queue.enqueue("Delta")
for i in range(4):
    print(queue.dequeue())

class Queue1:
    def __init__(self):
        self.q=[]
        self.maxSize = 100
        self.head=0;
        self.tail=0;
    def resetQueue(self):
        self.head=0;
        self.tail=0;
        self.q=[];
    def push(self, item):
        if((self.tail-self.head)>=self.maxSize):
        print("Queue is Full");
        q[self.tail]=item;
        self.tail=self.tail+1;
     
     #add code here
 
    def pop(self): 
        if((self.tail-self.head)<=0):
            self.resetQueue();
        print("Queue is empty");
        data = self.q[self.head];
        self.head=self.head+1;
     return data;