T = int(input())
for P in range(T):
    print("Case#{}: ".format(P+1),end='');
    no_of_pairs = int(input());
    cities = list(map(int, input().split()));
    no_of_queries = int(input());
    
    
    
    for each in range(no_of_queries):
        query = int(input());
        count=0;
        for doublejumpindex in range(0,len(cities),2):
#             print(doublejumpindex)
            if query in range(cities[doublejumpindex],cities[doublejumpindex+1]+1):
                count=count+1;
        print(count,end=' ')
         
    print()    
        